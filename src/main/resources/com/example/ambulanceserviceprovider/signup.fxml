<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ambulanceserviceprovider.controllers.SignUpController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Register">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <TextField maxWidth="500.0" prefHeight="25.0" prefWidth="363.0" promptText="Full Name" VBox.vgrow="NEVER" />
            <TextField maxWidth="500.0" prefHeight="25.0" prefWidth="363.0" promptText="Phone Number" VBox.vgrow="NEVER" />
            <TextField maxWidth="500.0" prefHeight="25.0" prefWidth="500.0" promptText="Email" VBox.vgrow="NEVER" />
            <ComboBox fx:id="userType" maxWidth="500.0" prefHeight="27.0" prefWidth="596.0" />
            <HBox alignment="CENTER" VBox.vgrow="NEVER">
               <children>
                  <Button mnemonicParsing="false" onAction="#goBackButton" text="Go Back" />
                  <Region maxWidth="372.0" HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#registerUserButton" text="Register" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
